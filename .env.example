# Starknet Configuration
STARKNET_ACCOUNT=~/.starkli-wallets/deployer/account.json
STARKNET_KEYSTORE=~/.starkli-wallets/deployer/keystore.json
STARKNET_RPC=https://starknet-sepolia.infura.io/v3/YOUR_API_KEY

# Calimero Configuration
CALIMERO_NODE_NAME=node1
CALIMERO_SERVER_PORT=2428
CALIMERO_SWARM_PORT=2528
CALIMERO_PROTOCOL=starknet
CALIMERO_API_KEY=your_api_key_here
CALIMERO_NETWORK_ID=your_network_id_here
CALIMERO_ENDPOINT=your_endpoint_here

# Contract Addresses (After deployment)
VOTING_SYSTEM_ADDRESS=
BALLOT_FACTORY_ADDRESS=

# Frontend Configuration
NEXT_PUBLIC_STARKNET_NETWORK=sepolia
NEXT_PUBLIC_CALIMERO_ENDPOINT=http://localhost:${CALIMERO_SERVER_PORT}

# Development
NODE_ENV=development
PORT=3000

# Security (Do not share these values)
ZK_PROOF_SALT=your_random_salt_here
VOTER_REGISTRY_SECRET=your_secret_here
